
{% if ('delete' in actions) and (not hide_delete) %}
    <a href="{{ "{{ path('" ~ route_name_prefix ~ "_delete', { 'id': entity.id }) }}" }}" class="btn btn-danger action-delete">
        Delete
    </a>
{% endif %}

{% if ('edit' in actions) and (not hide_edit) %}
    <a href="{{ "{{ path('" ~ route_name_prefix ~ "_edit', { 'id': entity.id }) }}" }}" class="btn btn-primary">
        Edit
    </a>
{% endif %}

<a href="{{ "{{ path('" ~ route_name_prefix ~ "') }}" }}" class="btn btn-primary">
    Back to the list
</a>